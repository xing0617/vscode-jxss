{
    "name": "vscode-jxss",
    "displayName": "jxss",
    "description": "京东小程序jxss高亮显示",
    "version": "0.0.3",
    "keywords": ["jd", "miniprogram", "jxss"],
    "publisher": "xing0617",
    "author": "xs",
    "repository": {
        "type": "git",
        "url": "https://github.com/xing0617/vscode-jxss.git"
    },
    "engines": {
        "vscode": "^1.27.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "jxss",
            "aliases": ["JXSS", "jxss"],
            "extensions": [".jxss"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "jxss",
            "scopeName": "source.jxss",
            "path": "./syntaxes/jxss.tmLanguage.json",
            "embeddedLanguages": {
                "source.css": "css"
            }
        }]
    }
}